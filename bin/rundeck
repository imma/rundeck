#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "${BLOCK_PATH:-"$shome/work"}/block/script/profile" ~
  source normalize  

  exec java ${RDECK_JVM_SETTINGS:-} -jar "$shome/vendor/rundeck-launcher-${RUNDECK_VERSION}.jar" "$@"

}

source sub "$BASH_SOURCE" "$@"
